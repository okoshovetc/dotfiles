alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ls="ls -F --color=auto"
alias la="ls -a"
alias ll="ls -l"
alias grep="grep --color"
alias cl="clear"
alias home="cd $HOME"
alias logs="~/scripts/colorizer.pl --config ~/scripts/logs.conf"
alias mylogs="color --config ~/dotfiles/custom_logs.conf"
alias color="~/dotfiles/colorizer.pl"
alias watcher="~/scripts/filewatcher.pl"
alias mygrep="color --config ~/scripts/mygrep.conf"
alias tmux="tmux -2 -u"
alias gotmux="~/gotmux.sh"
alias gus="git status"
alias sun="sort | uniq"
alias sunc="sort | uniq -c"
alias 3mon="xrandr --output DP-1 --above eDP-1 --mode 1920x1080 --output HDMI-2 --above DP-1 --mode 1920x1080"
alias 1mon="xrandr --output DP-1 --off --output HDMI-2 --off"
